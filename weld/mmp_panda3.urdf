    <child link= "base_x"/>
    <axis xyz = "1 0 0" />
    <calibration rising="0.0" />
    <limit effort="87" lower="-5" upper="5" velocity="0.2" />
  </joint>

  <joint name = "base_prismatic_y" type = "prismatic">
    <origin xyz = "0 0 0" rpy = "0 0 0"/>
